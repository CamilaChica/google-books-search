    "concurrently": "^4.1.0"
  },
  "dependencies": {
    "@sentry/browser": "^5.7.1",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.7.7"
